{
  "name": "proxy_server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc --skipLibCheck || true",
    "build:worker": "npx tsc --project tsconfig.worker.json --skipLibCheck || true",
    "start": "node dist/index.js",
    "deploy": "npm run build:worker && wrangler deploy --env=\"\"",
    "deploy:prod": "npm run build:worker && wrangler deploy --env production",
    "upload-db": "bash scripts/upload-databases.sh",
    "deploy:all": "npm run upload-db && npm run deploy"
  },
  "keywords": [],
  "author": "Akshat Kotpalliwar (alias IntegerAlex on GitHub)",
  "license": "GPL-3.0",
  "description": "",
  "dependencies": {
    "@maxmind/geoip2-node": "^6.0.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "hasty-server": "^0.9.52"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250101.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.9",
    "typescript": "^5.8.2",
    "wrangler": "^3.0.0"
  }
}
